{
    "env": {
        "BOLOS_SDK": "${workspaceFolder}/dev-env/SDK/nanos-secure-sdk",
        "BOLOS_ENV": "${workspaceFolder}/dev-env/CC/others"
    },
    "configurations": [
        {
            "name": "Nano S",
            "includePath": [
                "/usr/include",
                "${workspaceFolder}/src",
                "${env:BOLOS_SDK}/include/*",
                "${env:BOLOS_SDK}/lib_ux/include/*",
                "${env:BOLOS_SDK}/lib_cxng/include/*"
            ],
            "defines": [
                "TARGET_NANOS",
                "OS_IO_SEPROXYHAL",
                "HAVE_BAGL",
                "HAVE_SPRINTF",
                "HAVE_IO_USB",
                "HAVE_L4_USBLIB",
                "IO_USB_MAX_ENDPOINTS=6",
                "IO_HID_EP_LENGTH=64",
                "HAVE_USB_APDU",
                "USB_SEGMENT_SIZE=64",
                "UNUSED(x)=(void)x",
                "APPVERSION=\"1.0.0\"",
                "APPNAME=\"Hathor Dev\"",
                "MAJOR_VERSION=1",
                "MINOR_VERSION=0",
                "PATCH_VERSION=0",
                "IO_SEPROXYHAL_BUFFER_SIZE_B=128",
                "HAVE_UX_FLOW",
                "DEBUG=1",
                "HAVE_PRINTF",
                "PRINTF=screen_printf",
                // minimal from below
                // "HAVE_ECC",
                // "HAVE_SECP256K1_CURVE",
                // "HAVE_ECC_WEIERSTRASS",
                // "HAVE_HASH",
                // "HAVE_SHA3",
                // from Makefile.conf.cx (for completion)
                "HAVE_NES_CRYPT",
                "HAVE_ST_EDES_PLUS",
                "HAVE_ST_AES",
                "NATIVE_LITTLE_ENDIAN",
                "HAVE_CRC",
                "HAVE_HASH",
                "HAVE_RIPEMD160",
                "HAVE_SHA224",
                "HAVE_SHA256",
                "HAVE_SHA3",
                "HAVE_SHA384",
                "HAVE_SHA512",
                "HAVE_SHA512_WITH_BLOCK_ALT_METHOD",
                "HAVE_SHA512_WITH_BLOCK_ALT_METHOD_M0",
                "HAVE_BLAKE2",
                "HAVE_GROESTL",
                "HAVE_HMAC",
                "HAVE_PBKDF2",
                "HAVE_DES",
                "HAVE_AES",
                "HAVE_MATH",
                "HAVE_RNG",
                // "HAVE_RNG_NOPOSTPROCESS",
                "HAVE_RNG_RFC6979",
                "HAVE_RNG_SP800_90A",
                // #"HAVE_RNG_SONY",
                // #"HAVE_RSA",
                "HAVE_ECC",
                "HAVE_ECC_WEIERSTRASS",
                "HAVE_ECC_TWISTED_EDWARDS",
                "HAVE_ECC_MONTGOMERY",
                // #"HAVE_SECP_CURVES",
                "HAVE_SECP256K1_CURVE",
                "HAVE_SECP256R1_CURVE",
                "HAVE_SECP384R1_CURVE",
                "HAVE_SECP521R1_CURVE",
                // #"HAVE_ANSSI_CURVES",
                "HAVE_FR256V1_CURVE",
                // #"HAVE_STARK_CURVES",
                "HAVE_STARK256_CURVE",
                "HAVE_BLS12_381_G1_CURVE",
                // #"HAVE_BRAINPOOL_CURVES",
                "HAVE_BRAINPOOL_P256R1_CURVE",
                "HAVE_BRAINPOOL_P256T1_CURVE",
                "HAVE_BRAINPOOL_P320R1_CURVE",
                "HAVE_BRAINPOOL_P320T1_CURVE",
                "HAVE_BRAINPOOL_P384R1_CURVE",
                "HAVE_BRAINPOOL_P384T1_CURVE",
                "HAVE_BRAINPOOL_P512R1_CURVE",
                "HAVE_BRAINPOOL_P512T1_CURVE",
                // #"HAVE_CV_CURVES",
                "HAVE_CV25519_CURVE",
                "HAVE_CV448_CURVE",
                // #"HAVE_ED_CURVES",
                "HAVE_ED25519_CURVE",
                "HAVE_ED448_CURVE",
                "HAVE_ECDH",
                "HAVE_ECDSA",
                "HAVE_EDDSA",
                "HAVE_ECSCHNORR"
            ],
            "compilerPath": "${env:BOLOS_ENV}/gcc-arm-none-eabi-10-2020-q4-major/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}